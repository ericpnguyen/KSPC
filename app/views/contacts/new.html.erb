<%= form_for @contact, html:{multipart: true} do |f| %>
	<%= f.label :'First Name' %>
	<%= f.text_field :first %>
	<br>
	<%= f.label :'Last Name' %>
	<%= f.text_field :last %>
	<br>
	<%= f.label :email %>
	<%= f.text_field :email %>
	<br>
	<%= f.label :inquiry %>
	<%= f.text_field :inquiry %>
	<br>
	<%= f.label :comment %>
	<%= f.text_field :comment %>
	<br>
	<%= f.submit :'Submit'%>

<% end %>